package scripts.closet {	import com.smartfoxserver.v2.entities.data.ISFSObject;	import com.smartfoxserver.v2.entities.data.SFSObject;	import com.smartfoxserver.v2.requests.ExtensionRequest;	import flash.display.*;	import flash.events.*;	import flash.display.MovieClip;	import flash.display.Stage;	import flash.geom.ColorTransform;	import fl.motion.Color;;	//	public class closetColorSwatch extends MovieClip {		public var pNum:int = 0;		public var pType:String = "skin";		//		public function closetColorSwatch ():void {			addEventListener (MouseEvent.MOUSE_DOWN,mousedown);			var nme1:String = this.name;			var nme2:Array = nme1.split("_");			pNum = nme2[1];			pType = (this.parent.name);			stop ();		}		//		private function mousedown (evt:MouseEvent):void {			trace(parent.parent.name);			trace('switch ' + pType + ' to ' + pNum);			var hex:String = traceTint();			switch (pType){				case "skinSwatches":					root.changeAvatarTint("skin", pNum);					var obj:ISFSObject = new SFSObject();					obj.putUtfString("cmd2", "UpdateAvatarColor");					obj.putUtfString("AvatarColor", pNum);					parent.parent.pRoot.smartFox.send(new ExtensionRequest("set", obj));					break;				case "hairSwatches":					root.changeAvatarTint("hair", pNum);					obj = new SFSObject();					obj.putUtfString("cmd2", "UpdateHairColor");					obj.putUtfString("HairColor", pNum);					parent.parent.pRoot.smartFox.send(new ExtensionRequest("set", obj));					break;			}		}		//		private function traceTint():String {			var ct:ColorTransform = this.transform.colorTransform;			var c:int = ct.color;			var hex:String = ("0x" + (c.toString(16).toUpperCase()));			trace ("color Swatch: hex = " + hex);			return hex;		}		//	}}