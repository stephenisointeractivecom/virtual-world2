package scripts.closet{	import flash.display.*;	import flash.events.*;	import flash.display.MovieClip;	import flash.display.Stage;	//	public class closetIconBtn extends MovieClip {		private var pBtnNme:String="";		public var pSwf:String="";		public var pItemNum:int = 0;		public var pItemType:String = "";		private var pSelected:Boolean=false;		private var pOwned:Boolean=false;		//		public function closetIconBtn():void {			addEventListener(MouseEvent.MOUSE_DOWN,mousedown);			addEventListener(MouseEvent.ROLL_OVER,rollover);			this.addEventListener(MouseEvent.ROLL_OUT,rollout);			pBtnNme=this.name;			this.iconframe.stop();			stop();		}		//		public function clearGraphic():void {			var mc:Object=this.iconHolder;			if (mc.numChildren>0) {				mc.removeChildAt(0);			}			pSwf="";			pItemNum = 0;			pItemType "";		}		//		private function rollover(evt:MouseEvent):void {			this.iconframe.gotoAndStop(2);			//var txt:String = "item:      " + pSwf.split("_").join(" ");//			txt = txt + "\r" +  "price:    " + "100"//			txt = txt + "\r" +  "own:     " + "false"//			root.itemInfoTxt.text = txt;		}		private function rollout(evt:MouseEvent):void {			this.iconframe.gotoAndStop(1);			if (pOwned) {				this.iconframe.gotoAndStop(3);			}		}		//		private function mousedown(evt:MouseEvent):void {			root.iconBtnClick(pItemNum, pItemType);		}		//	}}