package scripts.loginPage{	import flash.display.*;	import flash.events.*;	import flash.display.MovieClip;	import flash.display.Stage;	//	public class savePwBtn extends MovieClip {		public var pSavePWToggle:int = -1;		//		public function savePwBtn ():void {			trace ("savePwBtn init");			addEventListener (MouseEvent.MOUSE_DOWN,mousedown);			stop ();		}		//		private function mousedown (evt:MouseEvent):void {			pSavePWToggle = pSavePWToggle * -1;			if (pSavePWToggle == 1){				toggleUp();			}else{				toggleDown();			}		}		//		public function setToggle (vState:Boolean):void{			if (vState){				toggleUp();			}else{				toggleDown();			}		}		//		private function toggleUp():void{			pSavePWToggle = 1;			root.pSavePWToggle = true;			this.gotoAndStop(2);		}		//		private function toggleDown():void{			pSavePWToggle = -1;			root.pSavePWToggle = false;;			this.gotoAndStop(1);		}		//	}}