package scripts.mapPage{	import flash.display.*;	import flash.events.*;	import flash.display.MovieClip;	import flash.display.Stage;	//	public class mapBtn extends MovieClip {		private var pBtnNme:String = "";		private var pToggle:int = -1;		//		public function mapBtn ():void {			addEventListener (MouseEvent.MOUSE_DOWN,mousedown);			addEventListener (MouseEvent.ROLL_OVER,rollover);			this.addEventListener (MouseEvent.ROLL_OUT,rollout);			pBtnNme = this.name;			stop ();		}		private function rollover (evt:MouseEvent):void {			this.gotoAndStop (2);		}		private function rollout (evt:MouseEvent):void {			this.gotoAndStop (1);		}		//		private function mousedown (evt:MouseEvent):void {			//trace (this);			if (root) {				var lx:int = 0;				var ly:int = 0;				//normal root buttons				switch (pBtnNme) {					case "meadow1":						lx = 100 + Math.random()*100;						ly  = 260;						root.pRoot.loadSideScrollScene ("meadow1", lx, ly);						break;					case "hearth":						lx = 100 + Math.random()*100;						ly  = 260;						root.pRoot.loadSideScrollScene ("hearth", lx, ly);						break;					case "luminarium":						lx = 200 + Math.random()*100;						ly= 700;						root.pRoot.loadSideScrollScene ("luminarium", lx, ly);						break;					case "universarium":						lx = 200 + Math.random()*100;						ly= 200;						root.pRoot.loadSideScrollScene ("universe1", lx, ly);						break;				}			}		}		//	}}