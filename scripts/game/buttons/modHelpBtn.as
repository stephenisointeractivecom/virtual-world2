package scripts.game.buttons{	import flash.display.*;	import flash.events.*;	import flash.display.MovieClip;	import flash.display.Stage;	//	public class modHelpBtn extends MovieClip {		private var pCycle = 1800;		//		public function modHelpBtn():void {			addEventListener(MouseEvent.MOUSE_DOWN,mousedown);			addEventListener(MouseEvent.ROLL_OVER,rollover);			this.addEventListener(MouseEvent.ROLL_OUT,rollout);			this.addEventListener(Event.ENTER_FRAME, enterframe);			stop();		}		private function rollover(evt:MouseEvent):void {			this.gotoAndStop(2);		}		private function rollout(evt:MouseEvent):void {			this.gotoAndStop(1);		}		//		private function mousedown(evt:MouseEvent):void {			if (pCycle < 1800){				return;			}			root.callModForHelp();			this.alpha = .5;			pCycle = 0;		}		//		private function enterframe (e:Event):void{			pCycle++			if (pCycle > 1800){				this.alpha = 1;			}		}		//	}}