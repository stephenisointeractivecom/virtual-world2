package scripts.game.sceneObjects{	import flash.display.Sprite;	import flash.events.*;	import flash.display.Stage;	import flash.geom.Point;	import flash.display.BitmapData;	import flash.geom.Rectangle;	//	public class waypoint extends Sprite {		private var pRoot:Object = null;		private var pRoomObj:Object = null;		private var pState:int = 1;		private var x1:int = 0;		private var y1:int = 0;		private var pName:String = "wp_0";		private var pDestination:String = "";		private var pCycle:int = 0;		//		public function waypoint():void {		}		//		public function init(vRoot:Object, vRoomObj:Object, vLayer:Object, pSwfTarget):void {			//trace("waypoint: init" + vRoomObj);			this.visible = false;			pRoot = vRoot;			pRoomObj = vRoomObj;			pName = this.name;			var vNameArray:Array = pName.split("_");			pDestination = vNameArray[1];			x1 = this.x;			y1 = this.y;		}		//		public function enterframe():void {			if (pRoomObj && pRoot.pMyAvatar) {				var x2 = pRoot.pMyAvatar.x1;				var y2 = pRoot.pMyAvatar.y1;				var distX:int = Math.abs(x2-x1);				if (distX<30) {					var distY:int = Math.abs(y2-y1)					if (distY<30) {						pCycle++						if (pCycle > 30){							pRoot.wayPointJump(pDestination, "waypoint");							deleteMe();						}					}else{						pCycle = 0;					}				}			}		}		//		public function deleteMe():void {			if (pRoot) {				trace ("waypoint: deleteMe");				pRoot = null;				pRoomObj = null;				pState = 0;			}		}		//	}}//