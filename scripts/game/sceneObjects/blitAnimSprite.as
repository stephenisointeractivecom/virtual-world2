package scripts.game.sceneObjects{	import flash.display.MovieClip;	import flash.events.*;	import flash.display.Stage;	import flash.geom.Point;	import flash.display.BitmapData;	import flash.geom.Rectangle;	//	public class blitAnimSprite extends MovieClip {		private var pRoot:Object = null;		private var pRoomObj:Object = null;		private var pRect:Rectangle = null;		private var pPnt:Point = new Point  ;		private var pSheetArray:Array = [];		private var pSheetDone:Boolean = false;		private var pFrm:int = 0;		private var pFrms:int = this.totalFrames;		private var pRandom = false;		//		public function blitAnimSprite():void {		}		//		public function init(vRoot:Object, vRoomObj:Object, vLayerObj:Object, vSwfTarget):void {			//trace ("hearthFireplace: init");			pRoot = vRoot;			pRoomObj = vRoomObj;			bmd = new BitmapData(this.width,this.height,true,0x00000000);			bmd.draw(this);			pRect = bmd.rect;			pPnt.x = this.x;			pPnt.y = this.y;			this.visible = false;			var nme:String = this.name;			var nmeArray = nme.split("_");			if (nmeArray[1] == "random"){				pRandom = true;			}			this.stop();		}		//		public function enterframe():void {			pFrm++;			if (pFrm > pFrms) {				pFrm = 0;			}			var bmd:BitmapData = null;			if (pSheetDone) {				if (pRandom) {					pFrm = pRoot.rand() * pFrms;				}				bmd = pSheetArray[pFrm];			} else {				bmd = new BitmapData(this.width,this.height,true,0x00000000);				bmd.draw(this);				pSheetArray.push(bmd);				if (pSheetArray.length > pFrms) {					trace("Bitted animation cache complete!");					pSheetDone = true;				}			}			pRoomObj.blitStaticSprite(bmd, pRect, pPnt);		}		//;		public function deleteMe():void {			if (pState !=-1) {				pRoot = null;				pRoomObj = null;				bmd = null;				pRect = null;				pState = -1;				pSheetArray = null;				if (parent) {					parent.removeChild(this);				}			}		}		//	}}//