package scripts.game.particles{	import flash.display.MovieClip;	import flash.events.*;	import flash.display.Stage;	//	public class portalBeam extends MovieClip {		private var pRoot:Object = null;		private var pRoomObj:Object = null;		private var pParentPortal:Object = null;		private var pCycle1:int = 0;		private var pState:int = 1;		private var pFrm:int = 0;		private var pFrmDir = 1;		private var x1:int = 0;		private var y1:int = 0;		//		public function portalBeam():void {		}		//		public function init(vRoot:Object, vRoomObj:Object, vParentPortal:Object ):void {			//trace ("Portal particle: init");			pRoot = vRoot;			pRoomObj = vRoomObj;			pParentPortal = vParentPortal;			x1 = this.x;			y1 = this.y;		}		//		public function enterframe():void {			pFrm++;			if (pFrm>24) {				pFrm = 1;				pCycle1++;				if (pCycle1>2) {					deleteMe();					return;				}			}			this.gotoAndStop(pFrm);		}		//		public function deleteMe():void {			//trace ("Portal particle: delete");			if (pState != -1) {				pRoot = null;				pRoomObj = null;				pState = -1;				if (parent) {					parent.removeChild(this);				}			}		}		//	}}//