package scripts.game.particles{	import flash.display.Sprite;	import flash.events.*;	import flash.display.Stage;	import flash.geom.Point;	import flash.display.BitmapData;	import flash.geom.Rectangle;	//	public class portalParticleA extends Sprite {		private var pRoot:Object = null;		private var pRoomObj:Object = null;		private var bmd:BitmapData;		private var pState:int = 1;		private var pFrm:int = 0;		private var x0:int = 0;		private var x1:int = 0;		private var y1:Number = 0;		private var dx:Number = 0;		private var dy:Number = 0;		private var regPnt:Point = new Point(0,0);		private var pRect:Rectangle = null;		private var pGravity:Number = .25;		//		public function portalParticleA():void {		}		//		public function init(vRoot:Object, vRoomObj:Object, vx1:int, vy1:int):void {			//trace ("Portal particle: init");			pRoot = vRoot;			pRoomObj = vRoomObj;			bmd = pRoot.pSpriteSheets["starSparkle"][0];			pRect = bmd.rect;			x0 = vx1;			x1 = vx1 + Math.round((pRoot.rand()-pRoot.rand()) * 20);			y1 = vy1 - Math.round(pRoot.rand() * 30);			dy = -1 * (1 + pRoot.rand() * 4);			dx = (Math.random() - Math.random())*5;		}		//		public function enterframe():void {			switch (pState) {				case 0 :					//					break;				case 1 :					moveIt();					blit();					break;			}		}		//		private function moveIt():void {			x1 = x1 + dx;			y1 = y1 + dy;			if (x1 > x0) {				dx = dx - .5;			} else {				dx = dx + .5;			}			dy = dy - .1;		}		//		private function blit():void {			pFrm++;			if (pFrm>9) {				pFrm = 0;			}			bmd = pRoot.pSpriteSheets["starSparkle"][pFrm];			if (bmd) {				var pnt:Point = new Point(Math.round(x1),Math.round(y1));				pRoomObj.blitSprite(bmd, pRect, pnt);			}			if (y1<20) {				deleteMe();			}		}		//		public function deleteMe():void {			//trace ("Portal particle: delete");			if (pState != -1) {				parent.removeChild(this);				pRoot = null;				pRoomObj = null;				pAvatar = null;				pState = -1;				pRect = null;				bmd = null;			}		}		//	}}//