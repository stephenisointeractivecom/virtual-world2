package scripts.game{	import flash.display.*;	import flash.events.*;	import flash.display.MovieClip;	import flash.display.Stage;	//	public class gameBtn extends MovieClip {		private var pBtnNme:String = "";		//		public function gameBtn ():void {			addEventListener (MouseEvent.MOUSE_DOWN,mousedown);			addEventListener (MouseEvent.ROLL_OVER,rollover);			this.addEventListener (MouseEvent.ROLL_OUT,rollout);			pBtnNme = this.name;			stop ();		}		private function rollover (evt:MouseEvent):void {			this.gotoAndStop (2);		}		private function rollout (evt:MouseEvent):void {			this.gotoAndStop (1);		}		//		private function mousedown (evt:MouseEvent):void {			//trace (pBtnNme, this, root, parent);			if (root) {				//normal root buttons				switch (pBtnNme) {					case "closeInterfaceBtn":					case "openInterfaceBtn":						root.toggleSceneInterface();						break;					case "modMsgCloseBtn":						root.closeModMsgWindow();						break;					case "chatLogBtn":						root.toggleChatLog();						break;					case "openModPanelBtn":						root.toggleModPanel();						break;					case "buyBtn":						root.LaunchPurchasing();						break;					case "buyBtn_v1":						//root.LaunchPurchasing(1);						root.LaunchNPC();						break;					case "buyBtn_v2":						root.LaunchPurchasing(2);						break;					case "callModForHelpBtn":						root.callModForHelp();						this.alpha = .5;						break;					case 'inventory_btn':						root.showInventory();						break;					case 'doorRoomMC':						//root.loadDormRoom();						root.loadSideScrollScene("dorm", 100 + Math.random() * 100, 410);						break;					case 'chromo_mc':						root.loadChromograph();						break;				}			}		}		//	}}