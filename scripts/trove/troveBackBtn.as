package scripts.trove{	import flash.display.*;	import flash.events.*;	import flash.display.MovieClip;	import flash.display.Stage;	//	public class troveBackBtn extends MovieClip {		private var pToggle:int = -1;		//		public function troveBackBtn():void {			addEventListener(MouseEvent.MOUSE_DOWN,mousedown);			addEventListener(MouseEvent.ROLL_OVER,rollover);			this.addEventListener(MouseEvent.ROLL_OUT,rollout);			stop();		}		private function rollover(evt:MouseEvent):void {			this.gotoAndStop(2);		}		private function rollout(evt:MouseEvent):void {			this.gotoAndStop(1);		}		//		private function mousedown(evt:MouseEvent):void {			lx = 500 + Math.random() * 50;			ly = 420;			root.pRoot.loadSideScrollScene("hearth", lx, ly);		}		//	}}