package scripts.parents.loginPage{	import flash.display.*;	import flash.events.*;	import flash.display.MovieClip;	import flash.display.Stage;	//	public class loginBtn extends MovieClip {		private var pBtnNme:String = "";		private var pToggle:int = -1;		//		public function loginBtn ():void {			addEventListener (MouseEvent.MOUSE_DOWN,mousedown);			addEventListener (MouseEvent.ROLL_OVER,rollover);			this.addEventListener (MouseEvent.ROLL_OUT,rollout);			pBtnNme = this.name;			stop ();		}		private function rollover (evt:MouseEvent):void {			this.gotoAndStop (2);		}		private function rollout (evt:MouseEvent):void {			this.gotoAndStop (1);		}		//		private function mousedown (evt:MouseEvent):void {			trace (pBtnNme);			if (root) {				//normal root buttons				switch (pBtnNme) {					case "loginBtn":						root.login ();						this.visible = false;						break;					case "guestloginBtn":						root.guestLogin();						this.visible = false;						break;					case "loginErrorBtn":						root.reLogin();						this.visible = false;						break;					case "submitModPwBtn":						root.submitModPw();						break;					case "registerBtn":						root.pRoot.loadRegistrationPage();						break;				}			}		}		//	}}