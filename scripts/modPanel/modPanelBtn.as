package scripts.modPanel{	import flash.display.*;	import flash.events.*;	import flash.display.MovieClip;	import flash.display.Stage;	//	public class modPanelBtn extends MovieClip {		private var pBtnNme:String = "";		private var pToggle:int = -1;		//		public function modPanelBtn():void {			addEventListener(MouseEvent.MOUSE_DOWN,mousedown);			this.addEventListener(MouseEvent.MOUSE_UP,mouseup);			addEventListener(MouseEvent.ROLL_OVER,rollover);			this.addEventListener(MouseEvent.ROLL_OUT,rollout);			pBtnNme = this.name;			this.btn.stop();			stop();		}		//		private function rollover(evt:MouseEvent):void {			this.btn.gotoAndStop(2);			this..gotoAndStop(2);		}		//		private function rollout(evt:MouseEvent):void {			this.btn.gotoAndStop(1);			this..gotoAndStop(2);		}		//		private function mousedown(evt:MouseEvent):void {			trace (pBtnNme);			this.btn.gotoAndStop(3);			if (root) {				//normal root buttons				switch (pBtnNme) {					case "closeModPanelBtn":						root.closeModPanel();						break;					case "moonsBtn" :						root.selectSubPanel("moons_subPanel");						break;					case "modBtn" :						root.selectSubPanel("mod_subPanel");						break;					case "alertBtn" :						root.selectSubPanel("alert_subPanel");						break;					case "helpBtn" :						root.selectSubPanel("help_subPanel");						break;						//					case "bootBtn" :						root.bootUser();						break;					case "banBtn" :						root.banUser();						break;					case "getUserInfoBtn":						root.getUserInfo();						break;					case "warnBtnRude" :						root.warnPreset("rude");						break;					case "warnBtnLewd" :						root.warnPreset("lewd");						break;					case "warnBtnRacist" :						root.warnPreset("racist");						break;					case "warnBtnSpam" :						root.warnPreset("spam");						break;					case "warnBtnAnnoy" :						root.warnPreset("annoy");						break;					case "warnBtnMean" :						root.warnPreset("mean");						break;					case "warnCustomBtn" :						root.warnCustom();						break;					case "sendRoomAnnounceBtn" :						root.sendAnnounce("Room");						break;					case "sendZoneAnnounceBtn" :						root.sendAnnounce("Zone");						break;					case "applyMoonPhaseBtn" :						root.applyMoonPhase();						break;				}			}		}		//		private function mouseup(evt: MouseEvent):void {			this.btn.gotoAndStop(2);			if (root) {				switch (pBtnNme) {				}			}		}		///	}}